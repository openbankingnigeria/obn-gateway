{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "GPL-3.0-only",
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "build": "NODE_EXTRA_CA_CERTS=./certs/ca/ca.crt nest build && copyfiles -u 1 src/**/*.html dist/src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_EXTRA_CA_CERTS=./certs/ca/ca.crt nest start",
    "dev": "NODE_EXTRA_CA_CERTS=./certs/ca/ca.crt nest start --watch",
    "start:debug": "NODE_EXTRA_CA_CERTS=./certs/ca/ca.crt nest start --debug --watch",
    "start:prod": "NODE_EXTRA_CA_CERTS=./certs/ca/ca.crt node dist/src/main",
    "start:setup": "NODE_EXTRA_CA_CERTS=./certs/ca/ca.crt node dist/src/setup",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:run": "TS_NODE_TRANSPILE_ONLY=true pnpm typeorm-ts-node-commonjs migration:run -d ./src/common/database/migration.config.ts",
    "migration:run:prod": "typeorm migration:run -d ./dist/src/common/database/migration.config.js",
    "migration:generate": "TS_NODE_TRANSPILE_ONLY=true pnpm typeorm-ts-node-commonjs migration:generate ./src/common/database/migrations/migration -d ./src/common/database/migration.config.ts -p",
    "migration:create": "TS_NODE_TRANSPILE_ONLY=true pnpm typeorm-ts-node-commonjs migration:create ./src/common/database/migrations/migration"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/elasticsearch": "^10.0.1",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "app-root-path": "^3.1.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "handlebars": "^4.7.8",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "mysql2": "^3.9.4",
    "nestjs-pino": "^3.5.0",
    "nodemailer": "^6.9.7",
    "pino-http": "^8.5.1",
    "pino-pretty": "^10.2.3",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "speakeasy": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^3.0.12",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-mock": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "ts-jest": "npm:ts-jest",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
